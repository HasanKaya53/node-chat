    </head>


    <body>

        <div class="container">

            <div class="mb-3">
                <label for="exampleFormControlInput1" class="form-label">Chat Room</label>
                <select type="text" class="form-control form-control-sm" id="roomName">
                    <option value="general">General</option>
                    <option value="test">Test</option>
                </select>
              </div>
            <div class="mb-3">
                <label for="exampleFormControlInput1" class="form-label">Chat Name</label>
                <input type="text" class="form-control form-control-sm" id="chatName" placeholder="">
                <!-- submit button-->
                <button type="button" class="btn btn-primary btn-sm mt-2" id="submit">Chat !</button>
              </div>
        </div>


    </body>



 
    <script src="http://localhost:3000/socket.io/socket.io.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
